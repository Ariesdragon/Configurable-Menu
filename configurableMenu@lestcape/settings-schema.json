        "type": "combobox",
        "description": "Menu layout:",
        "default": "classic",
        "tooltip": "Select a layout of menu.",
        "options": {
            "Classic": "classic",
            "GnoMenu Left": "gnomenuLeft",
            "GnoMenu Right": "gnomenuRight",
